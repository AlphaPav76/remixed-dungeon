    <!-- Mobs descriptions -->
    <!-- STRINGS OF Kobold CLASS -->
    <string name="Kobold_Name">狗头人</string>
    <string name="Kobold_Gender">男性的</string>
    <string name="Kobold_Name_Objective">狗头人</string>
    <string name="Kobold_Desc">狗头人被认为是一个传说。虽然它们和豺狼人有着同样的祖先，但是它们居住在冰霜洞穴中。同时它们也有很高的工程机械造诣。但没人知道为什么它们消失了这么多年...</string>

    <!-- STRINGS OF KoboldIcemancer CLASS -->
    <string name="KoboldIcemancer_Name">狗头人冰结</string>
    <string name="KoboldIcemancer_Gender">masculine</string>
    <string name="KoboldIcemancer_Name_Objective">狗头人冰结师</string>
    <string name="KoboldIcemancer_Desc"> 狗头人冰结师是狗头人中的精英。它们既是战士，也是法师，更是科学家。冰结师们会用它们先进的理论和实践经验来加强它们的冰系魔法...</string>
    <string name="KoboldIcemancer_Killed">%s的寒冰箭击杀了你...</string>

    <!-- STRINGS OF ColdSpirit CLASS -->
    <string name="ColdSpirit_Name">寒冰精灵</string>
    <string name="ColdSpirit_Gender">masculine</string>
    <string name="ColdSpirit_Name_Objective">寒冰精灵</string>
    <string name="ColdSpirit_Desc">寒冰精灵是由纯粹的寒冷和冷酷所构成的。它们被它们的创造者造成充满魔法的空壳，听从创造者的指令或者根据本能来行动。</string>

    <!-- STRINGS OF IceElemental CLASS -->
    <string name="IceElemental_Name">霜元素</string>
    <string name="IceElemental_Gender">masculine</string>
    <string name="IceElemental_Name_Objective">霜元素</string>
    <string name="IceElemental_Desc">虽然说地牢中的所有元素中霜元素不是最难遇到的，但是它往往被认为是最危险的。因为它很冷，而且它的攻击可以减速敌人。</string>

    <!-- STRINGS OF IceGuardian CLASS -->
    <string name="IceGuardian_Name">冰霜巨人</string>
    <string name="IceGuardian_Gender">masculine</string>
    <string name="IceGuardian_Name_Objective">冰霜巨人</string>
    <string name="IceGuardian_Desc">冰霜巨人是被狗头人所建造的为保护它们的洞穴的古老产物。它的核心允许它在接受足够的能量后重生。</string>

    <!-- STRINGS OF IceGuardianCore CLASS -->
    <string name="IceGuardianCore_Name">冰霜巨人</string>
    <string name="IceGuardianCore_Gender">masculine</string>
    <string name="IceGuardianCore_Name_Objective">冰霜巨人</string>
    <string name="IceGuardianCore_Desc">冰霜巨人是被狗头人所建造的为保护它们的洞穴的古老产物。它的核心允许它在接受足够的能量后重生。</string>

    <!-- STRINGS OF CagedKobold CLASS -->
    <string name="CagedKobold_Name">被囚禁的狗头人</string>
    <string name="CagedKobold_Gender">masculine</string>
    <string name="CagedKobold_Name_Objective">被囚禁的狗头人</string>
    <string name="CagedKobold_Desc">一个在笼子里的狗头人。</string>
    <string name="CagedKobold_Intro">喂...喂！你在这里冒险吗？请帮帮我，我的族人想用那台古老的机器来征服人类！因为那样做从各个方面说实在太危险了，所以我提出了反对意见，但是它们却把我给囚禁在这里。请帮助我找到这个笼子的钥匙。作为回报我会尽我所能的帮助你。</string>
    <string name="CagedKobold_Quest_End">哦，谢谢，太感谢了！你是怎么找到这把钥匙的？你...拆了那个武器？！令人吃惊！好吧，我说过我要帮助你，所以我打算跟随你一整子。*被囚禁的狗头人变成了一根蜡烛</string>
    <string name="CagedKobold_Message1">请...</string>
    <string name="CagedKobold_Message2">你要帮我出来，伙计</string>
    <string name="CagedKobold_Message3">我讨厌这个笼子</string>

    <string name="Journal_Caged_Kobold">被囚禁的狗头人</string>

