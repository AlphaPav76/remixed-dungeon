  <!--PACKAGE /items/scrolls-->
  <!--STRINGS OF Scroll CLASS-->
  <string name="Scroll_Name">\"%s\" 두루마리</string>
  <string name="Scroll_Info">이 양피지는 판독할 수 없는 룬 문자 %s로 덮여있습니다. 누가 이것을 소리내어 읽을때 어떤 일이 일어날까요?</string>
  <string name="Scroll_ACRead">읽는다</string>
  <string name="Scroll_Blinded">눈이 먼 상태에서는 두루마리를 읽을 수 없습니다.</string>
  <string-array name="Scroll_Runes">
    <item>카우난</item>
