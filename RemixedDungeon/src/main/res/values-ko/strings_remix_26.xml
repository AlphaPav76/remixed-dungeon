    <!-- Items descriptions -->
    <!-- STRINGS OF ChristmasTurkey CLASS -->
    <string name="ChristmasTurkey_Name">축제 칠면조</string>
    <string name="ChristmasTurkey_Info">던전에 몬스터가 침공했기 때문에 전세계의 모험가와 영웅을 끌어들여서 관광사업을 촉진하고 지역 상업에 이득을 준 날을 기념하는 던전마스를 축하하기 위한 가장 좋은 방법은 구운 칠면조를 먹는 것입니다. 당신도 가능합니다.  </string>

    <!-- STRINGS OF IceKey CLASS -->
    <string name="IceKey_Name">얼음 열쇠</string>
    <string name="IceKey_Info">이 열쇠는 얼음으로 만들어진 것 같다. 하지만 강철처럼 단단하다.</string>


    <!-- STRINGS OF WandOfIcebolt CLASS -->
    <string name="WandOfIcebolt_Name">얼음 화살의 마법봉</string>
    <string name="WandOfIcebolt_Info">이 마법봉은 얼음 화살을 발사해 대상 생물체에게 약한 피해를 입힙니다.  또한 대상을 감속시키거나 빙결시킬 수도 있습니다. </string>
    <string name="WandOfIcebolt_Info1">당신은 자기 자신을 얼음 화살의 마법봉으로 죽여버렸습니다..</string>

